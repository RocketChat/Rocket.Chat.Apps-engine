{
    "extends": ["@rocket.chat/eslint-config"],
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-console": "off",
        "no-unused-vars": "error"
    }
}

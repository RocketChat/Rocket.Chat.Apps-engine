{
  "extends": "tslint:recommended",
  "rules": {
    "no-empty": false,
    "array-type": [true, "generic"],
    "no-console": [false],
    "no-duplicate-variable": true,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "max-line-length": [true, {
        "limit": 160,
        "ignore-pattern": "^import | *export .*? {"
    }]
  },
  "jsRules": {
    "quotemark": [true, "single"]
  }
}
